{
  "name": "newsletter-template",
  "version": "1.0.0",
  "main": "dist/src/build.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"{src,test,public}/**/*.ts\" --fix",
    "start": "dotenv -e .env -- node ./dist/server.js",
    "build": "webpack --config webpack.server.js",
    "dev": "dotenv -e .env -- ts-node-dev -r tsconfig-paths/register --respawn --ignore-watch node_modules --no-notify --exit-child src/main.ts",
    "test": "dotenv -e .env.test -- jest",
    "test:e2e": "dotenv -e .env.test -- jest --config ./jest.e2e.config.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.31.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.46.0",
    "express": "^4.18.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/supertest": "^2.0.12",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.2",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
